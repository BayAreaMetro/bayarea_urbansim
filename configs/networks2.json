{
  "model": "networks",
  "writetotmp": "nodes_prices",
  "writetocsv": "nodes_prices.csv",
  "networks": {
    "filenames": ["osm_bayarea.jar"],
    "factors": [1.0],
    "maxdistances": [2000],
    "twoway": [1]
  },
  "var_lib": {
    "ave_residential_price": "NETWORKS.accvar(dset.buildings,1500,vname='res_sales_price',agg='AGG_AVE',decay='DECAY_FLAT')",
    "ave_retail_rent": "NETWORKS.accvar(dset.buildings[dset.buildings.general_type=='Retail'],1500,vname='nonresidential_rent',agg='AGG_AVE',decay='DECAY_FLAT')",
    "ave_office_rent": "NETWORKS.accvar(dset.buildings[dset.buildings.general_type=='Office'],1500,vname='nonresidential_rent',agg='AGG_AVE',decay='DECAY_FLAT')",
    "ave_industrial_rent": "NETWORKS.accvar(dset.buildings[dset.buildings.general_type=='Industrial'],1500,vname='nonresidential_rent',agg='AGG_AVE',decay='DECAY_FLAT')"
  },
  "ind_vars": ["ave_residential_price","ave_retail_rent","ave_office_rent","ave_industrial_rent"],
  "show": true
}
