{
  "table": "dset.apartments",
  "internalname": "units",
  "model": "hedonicmodel",
  "table_sim": "dset.building_filter(residential=1)",
  "merge": {
    "table":"dset.nodes",
    "left_on":"_node_id",
    "right_index":true
  },
  "dep_var": "rent",
  "dep_var_transform": "np.log",
    "patsy": "np.log1p(unit_sqft) + sum_residential_units + ave_unit_sqft + ave_lot_sqft + ave_income + poor + jobs + sfdu + renters",
  "add_constant": true,
  "output_table": "dset.buildings",
  "output_varname": "res_rent",
  "output_transform": "np.exp"
}
