{
  "table": "dset.fetch_households(tenure='sales')",
  "est_sample_size": 10000,
  "model": "locationchoicemodel",
  "modeldependencies": "rsh.json",
  "alternatives": "dset.building_filter(residential=1).dropna(subset=['unit_sqft'])",
  "merge": {
    "table":"dset.fetch_csv('nodes.csv',index_col='node_id')",
    "left_on":"_node_id",
    "right_index":true
  },
  "sample_size": 100,
  "dep_var": "building_id",
  "segment": ["income_quartile"],
  "patsy": "np.log1p(unit_sqft) + sum_residential_units + ave_unit_sqft + ave_lot_sqft + ave_income + poor + jobs + sfdu + renters + I(np.log1p(income) * ave_income) - 1",
  "output_varname": "sales_household_building_ids",
  "web_service": false
}
