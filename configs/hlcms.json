{
  "table": "dset.households",
  "internalname": "households",
  "filters": ["households.tenure<>1"],
  "est_sample_size": 10000,
  "model": "locationchoicemodel",
  "modeldependencies": "rsh_simulate",
  "alternatives": "dset.building_filter(residential=1)",
  "NOTUSED":"dset.fetch_csv('nodes.csv',index_col='node_id')",
  "merge": {
    "table":"dset.nodes", 
    "left_on":"_node_id",
    "right_index":true
  },
  "sample_size": 100,
  "dep_var": "building_id",
  "segment": ["income_quartile"],
  "patsy": "np.log1p(unit_sqft) + sum_residential_units + ave_unit_sqft + ave_lot_sqft + ave_income + poor + sfdu + renters + np.log1p(res_sales_price) - 1",
  "relocation_rate": 0.04,
  "supply_constraint": "dset.building_filter(residential=1).residential_units.sub(dset.households.groupby('building_id').size(),fill_value=0)",
  "output_table": "dset.households",
  "output_varname": "owner_building_ids"
}
