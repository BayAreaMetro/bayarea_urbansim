{
  "table": "dset.nets",
  "internalname": "jobs",
  "model": "locationchoicemodel",
  "modeldependencies": "nrh.json",
  "alternatives": "dset.building_filter(residential=0)",
  "merge": {
    "table":"dset.nodes", 
    "left_on":"_node_id",
    "right_index":true
  },
  "sample_size": 100,
  "dep_var": "building_id",
  "segment": ["naics11cat"],
  "patsy": "np.log1p(stories) + ave_income + poor + sum_nonresidential_sqft + jobs + sfdu + renters + np.log1p(nonresidential_rent) - 1",
  "relocation_rate": 0.1,
  "supply_constraint": "dset.building_filter(residential=0).non_residential_units.sub(dset.nets.groupby('building_id').size(),fill_value=0)",
  "output_table": "dset.nets",
  "output_varname": "firms_building_ids"
}
